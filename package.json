{
  "name": "@esm-bundle/angular__upgrade",
  "version": "17.1.0",
  "description": "Repo to test esm-bundle's automatic publishing",
  "type": "module",
  "exports": {
    ".": {
      "es2022": "./es2022/ivy/angular-upgrade.js",
      "default": "./es2022/ivy/angular-upgrade.js"
    },
    "./static": {
      "es2022": "./es2022/ivy/angular-static.js",
      "default": "./es2022/ivy/angular-static.js"
    }
  },
  "scripts": {
    "test": "pnpm run build && concurrently -n w: 'pnpm:test:*'",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "debug:test:browser": "karma start karma.conf.cjs",
    "test:formatting": "prettier --check .",
    "build": "rimraf system es2022 && rollup -c",
    "format": "prettier --write .",
    "release": "release-it",
    "prepublishOnly": "pnpm run build && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "files": [
    "esm",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/angular__upgrade.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esm-bundle/angular__upgrade/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/esm-bundle/angular__upgrade#readme",
  "devDependencies": {
    "@angular/common": "17.1.0",
    "@angular/compiler": "17.1.0",
    "@angular/compiler-cli": "17.1.0",
    "@angular/core": "17.1.0",
    "@angular/platform-browser": "17.1.0",
    "@angular/platform-browser-dynamic": "17.1.0",
    "@angular/upgrade": "17.1.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "concurrently": "8.2.2",
    "husky": "8.0.3",
    "karma": "6.4.2",
    "karma-firefox-launcher": "2.1.2",
    "karma-jasmine": "5.1.0",
    "pinst": "3.0.0",
    "prettier": "3.0.0",
    "release-it": "17.0.1",
    "release-it-plugin-esm-bundle": "3.0.0",
    "rimraf": "5.0.5",
    "rollup": "4.9.5",
    "rxjs": "7.8.1",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "overrides": {
      "terser": "5.27.0"
    }
  }
}
